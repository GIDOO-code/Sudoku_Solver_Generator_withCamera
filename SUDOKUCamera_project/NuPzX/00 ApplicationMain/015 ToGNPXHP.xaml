        <Label x:Name="LblExpireDate" Content="Available date: 2015/01/10" HorizontalAlignment="Left" Height="26" Margin="37,53,0,0" VerticalAlignment="Top" Width="236" ContentStringFormat="" Foreground="White" FontSize="16" FontWeight="Bold"/>
        <Label x:Name="WarpToHP" Content="Warp to GNPX-HP after 5 seconds" HorizontalAlignment="Left" Height="27" Margin="24,101,0,0" VerticalAlignment="Top" Width="268" ContentStringFormat="" Foreground="White" FontSize="16" FontFamily="MS Mincho" FontWeight="Bold"/>

    </Grid>
